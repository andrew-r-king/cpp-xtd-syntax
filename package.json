{
	"name": "cpp-xtd-syntax",
	"displayName": "C++ Extended Syntax Highlighting",
	"description": "Alternative & Additional Syntax Highlighting for C++",
	"version": "1.0.0",
	"publisher": "catbytes",
	"repository": {
		"type": "git",
		"url": "https://github.com/andrew-r-king/rew.cpp-xtd-syntax"
	},
	"scripts": {
		"build": "ts-node ./build.ts"
	},
	"icon": "icon.png",
	"engines": {
		"vscode": "*"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "cpp",
				"extensions": [
					".c",
					".h.in",
					".xpm",
					".cc",
					".cpp",
					".cp",
					".cxx",
					".c++",
					".cu",
					".cuh",
					".h",
					".hh",
					".hpp",
					".hxx",
					".h++",
					".inl",
					".ino",
					".ipp",
					".tcc",
					".tpp"
				],
				"aliases": [
					"Cool C++"
				]
			},
			{
				"id": "rc",
				"extensions": [
					".rc"
				],
				"aliases": [
					"Rc"
				]
			}
		],
		"grammars": [
			{
				"language": "cpp",
				"scopeName": "source.cpp",
				"path": "./syntaxes/c_cpp_experimental.json"
			}
		]
	},
	"devDependencies": {
		"@types/js-yaml": "^3.12.1",
		"@types/node": "^12.0.12",
		"js-yaml": "^3.13.1",
		"ts-node": "^8.3.0",
		"typescript": "^3.5.2"
	}
}
